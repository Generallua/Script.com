local P,U,R,S,C=game:GetService("Players"),game:GetService("UserInputService"),game:GetService("RunService"),game:GetService("StarterGui"),workspace.CurrentCamera local pl=P.LocalPlayer if pl:FindFirstChild("PlayerGui") and pl.PlayerGui:FindFirstChild("RacingControlGUI")then pl.PlayerGui.RacingControlGUI:Destroy()end local g=Instance.new("ScreenGui") g.Name="RacingControlGUI" g.ResetOnSpawn=false g.Parent=game:GetService("CoreGui") local m=Instance.new("Frame",g) m.Size=UDim2.fromOffset(300,135) m.BackgroundColor3=Color3.fromRGB(255,248,231) m.BorderSizePixel=0 m.Active=true Instance.new("UICorner",m).CornerRadius=UDim.new(0,16) local st=Instance.new("UIStroke",m) st.Color=Color3.fromRGB(230,210,180) st.Thickness=1.5 local function cl(p)local v=C.ViewportSize return UDim2.fromOffset(math.clamp(p.X.Offset,0,v.X-m.Size.X.Offset),math.clamp(p.Y.Offset,0,v.Y-m.Size.Y.Offset))end m.Position=cl(UDim2.fromOffset(C.ViewportSize.X-330,C.ViewportSize.Y-200)) local lk=true local cs=0 local lck=Instance.new("TextButton",m) lck.Size=UDim2.fromOffset(22,22) lck.Position=UDim2.fromOffset(260,6) lck.Text="ðŸ”’" lck.TextScaled=true lck.BackgroundTransparency=1 lck.TextTransparency=0.4 local cb=Instance.new("TextButton",m) cb.Size=UDim2.fromOffset(26,26) cb.Position=UDim2.fromOffset(-22,6) cb.Text="X" cb.Font=Enum.Font.GothamBold cb.TextScaled=true cb.TextColor3=Color3.fromRGB(120,60,40) cb.BackgroundTransparency=1 cb.Visible=false lck.MouseButton1Click:Connect(function() lk=not lk lck.Text=lk and "ðŸ”’" or "ðŸ”“" cb.Visible=not lk end) cb.MouseButton1Click:Connect(function() cs=cs+1 if cs==1 then pcall(function()S:SetCore("SendNotification",{Title="Warning",Text="Are you sure you want to close?",Duration=3})end) task.delay(3,function()cs=0 end) else g:Destroy() end end) local dr=false local ds,sp m.InputBegan:Connect(function(i) if lk then return end if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then dr=true ds=i.Position sp=m.Position end end) U.InputChanged:Connect(function(i) if dr and not lk and(i.UserInputType==Enum.UserInputType.MouseMovement or i.UserInputType==Enum.UserInputType.Touch)then local d=i.Position-ds m.Position=cl(UDim2.fromOffset(sp.X.Offset+d.X,sp.Y.Offset+d.Y))end end) U.InputEnded:Connect(function(i) if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then dr=false end end) local function mkBox(x,v,h)local t=Instance.new("TextBox",m) t.Size=UDim2.fromOffset(90,24) t.Position=UDim2.fromOffset(x,8) t.BackgroundColor3=Color3.fromRGB(245,230,200) t.TextColor3=Color3.fromRGB(74,74,74) t.PlaceholderColor3=Color3.fromRGB(120,110,100) t.BorderSizePixel=0 t.ClearTextOnFocus=false t.Font=Enum.Font.Gotham t.TextSize=13 t.Text=tostring(v) t.PlaceholderText=h Instance.new("UICorner",t).CornerRadius=UDim.new(0,8) return t end local sb=mkBox(20,200,"MaxSpeed") local pb=mkBox(120,2,"Force") local tB,bB=false,false local function pedal(x,s,bg)local bt=Instance.new("TextButton",m) bt.Size=UDim2.fromOffset(90,90) bt.Position=UDim2.fromOffset(x,35) bt.Text=s bt.TextScaled=true bt.Font=Enum.Font.SourceSansBold bt.TextColor3=Color3.fromRGB(74,74,74) bt.BackgroundColor3=bg bt.BorderSizePixel=0 bt.AutoButtonColor=false Instance.new("UICorner",bt).CornerRadius=UDim.new(0,20) bt.InputBegan:Connect(function(i) if i.UserInputType==Enum.UserInputType.Touch or i.UserInputType==Enum.UserInputType.MouseButton1 then bt.BackgroundTransparency=0.3 if s=="O" then tB=true else bB=true end end end) bt.InputEnded:Connect(function(i) if i.UserInputType==Enum.UserInputType.Touch or i.UserInputType==Enum.UserInputType.MouseButton1 then bt.BackgroundTransparency=0 if s=="O" then tB=false else bB=false end end end) return bt end local rev=pedal(20,"-",Color3.fromRGB(230,210,180)) local acc=pedal(120,"O",Color3.fromRGB(210,190,160)) local gp=Instance.new("Frame",m) gp.Size=UDim2.fromOffset(50,90) gp.Position=UDim2.fromOffset(230,35) gp.BackgroundColor3=Color3.fromRGB(230,210,180) gp.BorderSizePixel=0 Instance.new("UICorner",gp).CornerRadius=UDim.new(0,16) local function gBtn(y,t)local b=Instance.new("TextButton",gp) b.Size=UDim2.fromOffset(38,38) b.Position=UDim2.fromOffset(6,y) b.Text=t b.TextScaled=true b.Font=Enum.Font.GothamBold b.TextColor3=Color3.fromRGB(74,74,74) b.BackgroundColor3=Color3.fromRGB(245,230,200) b.BorderSizePixel=0 b.AutoButtonColor=false Instance.new("UICorner",b).CornerRadius=UDim.new(0,12) b.InputBegan:Connect(function(i) if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then b.BackgroundTransparency=0.3 local v=tonumber(pb.Text)or 0 if t=="+"then pb.Text=tostring(v+1)elseif t=="âˆ’"then pb.Text=tostring(math.max(v-1,0))end end end) b.InputEnded:Connect(function(i) if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then b.BackgroundTransparency=0 end end) return b end local gu=gBtn(6,"+") local gd=gBtn(46,"âˆ’") R.RenderStepped:Connect(function() local c=pl.Character local r=c and c:FindFirstChild("HumanoidRootPart") if r then local v=r.Velocity local fv=Vector3.new(v.X,0,v.Z) local fp=tonumber(pb.Text)or 0 local ms=tonumber(sb.Text)or 0 if tB then local d=Vector3.new(r.CFrame.LookVector.X,0,r.CFrame.LookVector.Z) d=d.Magnitude>0 and d.Unit or Vector3.new(0,0,0) local nv=fv+d*fp if ms>0 and nv.Magnitude>ms then nv=nv.Unit*ms end r.Velocity=Vector3.new(nv.X,v.Y,nv.Z) end if bB then local d=Vector3.new(r.CFrame.LookVector.X,0,r.CFrame.LookVector.Z) d=d.Magnitude>0 and d.Unit or Vector3.new(0,0,0) local nv=fv-d*fp if ms>0 and nv.Magnitude>ms then nv=nv.Unit*ms end r.Velocity=Vector3.new(nv.X,v.Y,nv.Z) end end end) local J1,J2,J3={},0,"abc" for i=1,5 do table.insert(J1,i*2) J2=J2+i J3=J3..i end local function F(a,b)local x,y=a*b,b^2 return x+y end local DL=0 while DL<3 do DL=DL+1 end local dummyVar="neat";local unused={1,2,3};for i=1,3 do local x=i*i;dummyVar=dummyVar..x end
